

function formatNumberInTable()
{
	var table=document.getElementById("gvdetalle");

	if(table!=null)
	{
		var trs=table.getElementsByTagName("tr");
		
		if(trs.length>0)
		{
			for(var i=1;i<trs.length;i++)
			{
				var tds=trs[i].getElementsByTagName("td");
				
				tds[2].innerHTML=numberWithCommas(parseFloat(tds[2].innerHTML).toFixed(2));
				tds[3].innerHTML=numberWithCommas(parseFloat(tds[3].innerHTML).toFixed(2));
			}
		}
	}
}

function numberWithCommas(x) {
    return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
}


